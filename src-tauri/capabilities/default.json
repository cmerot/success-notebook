{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "store:allow-load",
    "store:allow-set",
    "store:allow-get",
    "store:allow-save",
    "store:allow-entries",
    "store:allow-delete",
    "store:allow-clear",
    "dialog:default",
    "fs:default",
    "fs:allow-app-write",
    "statusbar:default"
  ]
}